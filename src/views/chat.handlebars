<div class="chatBody">
    <h1 class="chatTitle">Websocket Chat</h1>

    <div class="chatContainer">
        <header>
            <h3 class="userName"></h3>
            <p class="typing"></p>
        </header>
        <main>
            <div class="chatMessage"></div>
        </main>
        <footer>
            <input type="text" placeholder="Message" id="inputMessage" />
            <button id="btnMessage">Send</button>
        </footer>
    </div>
</div>